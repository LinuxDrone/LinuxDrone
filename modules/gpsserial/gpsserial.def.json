{
    "type": "module_def",
    "name": "GpsSerial",
    "version": 1,
    "rt-priority": 66,
    "main-task-period": 150,
    "paramsDefinitions": [
        {
            "name": "Device Name",
            "description": {
                "ru": "Название девайса",
		"en": "GPS Serial Device"
            },
            "type": "const char*",
            "defaultValue": "GPS Serial",
            "validValues" : ["GPS Serial"],
            "unitMeasured": ""
        },
        {
            "name": "Serial Port",
            "description": {
                "ru": "Название девайса"
            },
            "type": "const char*",
            "defaultValue": "/dev/ttyO2",
            "validValues" : ["/dev/ttyO2","/dev/ttyO4","/dev/ttyO5"],
            "unitMeasured": ""
        },
        {
            "name": "Serial Type",
            "description": {
                "ru": "Название девайса"
            },
            "type": "const char*",
            "defaultValue": "UART",
            "validValues" : ["UART","RTDM"],
            "unitMeasured": ""
        },
        {
            "name": "Serial Speed",
            "description": {
                "ru": "Название девайса"
            },
            "type": "float",
            "defaultValue": 57600,
            "validValues" : [4800,9600,19200,38400,57600,115200],
            "unitMeasured": "int"
        },
        {
            "name": "GPS Protocol",
            "description": {
                "ru": "Название девайса"
            },
            "type": "const char*",
            "defaultValue": "NMEA SiRF",
            "validValues" : ["NMEA","NMEA SiRF","NMEA uBlox","Binary SiRF","Binary uBlox"],
            "unitMeasured": ""
        }
    ],
    "outputs": [
        {
            "name": "Coordinates",
            "Schema": {
                "type": "object",
                "id": "http://jsonschema.net",
                "properties": {
                    "latitude": {
                        "type": "float",
                        "required": true
                    },
                    "longitude": {
                        "type": "float",
                        "required": true
                    },
                    "altitude": {
                        "type": "float",
                        "required": true
		    },
                    "time": {
                        "type": "const char*",
                        "required": true
		    },
                    "fix": {
                        "type": "float",
                        "required": true
		    },
                    "satellites": {
                        "type": "float",
                        "required": true
		    }			
                }
            }
        }
    ]
}
