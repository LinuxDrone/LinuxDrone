{
    "type": "module_def",
    "name": "Ms5611",  
    "version": 2,
    "rt-priority": 80,
    "main-task-period": 200,
    "paramsDefinitions": [
        {
            "name": "I2C Device",
            "displayName": {
                "en": "Bus Name",
                "ru": "Имя шины"
            },
            "description": {
                "ru": "Название девайса"
            },
            "type": "const char*",
            "defaultValue": "/dev/i2c-1",
            "validValues" : ["/dev/i2c-1","/dev/i2c-2"],
            "unitMeasured": ""
        }
    ],
    "outputs":[
        {
            "name": "Pressure+Temp",
            "Schema":{
                "type":"object",
                "id": "http://jsonschema.net",
                "properties":{
                    "pressure": {
                        "type":"float",
                        "required":true
                    },
                    "temperature": {
                        "type":"float",
                        "required":true
                    }
                }
            }
        }
    ]
}
