{
  "type": "module_def",
  "name": "c-ms5611",
  "version": 2,
  "rt-priority": 80,
  "main-task-period": 2000,
  "transfer-task-period": 200000,
  "description": {
    "en": "Pressure sensor",
    "ru": "Датчик абсолютного давления"
  },
  "paramsDefinitions": [
    {
      "name": "I2C_Device",
      "displayName": {
        "en": "Bus Name",
        "ru": "Имя шины"
      },
      "description": {
        "en": "Path to i2c device",
        "ru": "Путь к файлу i2c устройства"
      },
      "type": "const char*",
      "defaultValue": "/dev/i2c-1",
      "validValues": ["/dev/i2c-1","/dev/i2c-2"],
      "unitMeasured": ""
    },
    {
      "name": "I2C_Address",
      "displayName": {
        "en": "Device address",
        "ru": "Адресс датчика на шине"
      },
      "description": {
        "en": "Device address",
        "ru": "Адресс датчика на шине"
      },
      "type": "char",
      "defaultValue": 119,
      "validValues": [118,119],
      "unitMeasured": ""
    },
    {
      "name": "OverSampling",
      "displayName": {
        "en": "Ovesampling",
        "ru": "Передискретизация"
      },
      "description": {
        "en": "Affect the accuracy of measurement",
        "ru": "Влияет на точность измерения"
      },
      "type": "int",
      "defaultValue": 4096,
      "validValues": [256,512,1024,2048,4096],
      "unitMeasured": "int"
    },
    {
      "name": "calcPress0",
      "displayName": {
        "en": "calc Pressure0",
        "ru": "Расчет Pressure0 для высотометра"
      },
      "description": {
        "en": "Enable calc Pressure0",
        "ru": "Включить рассчет Pressure0 для высотометра"
      },
      "type": "bool",
      "defaultValue": false,
      "validValues": [false, true],
      "unitMeasured": "bool"
    },
    {
      "name": "alfaFiltered",
      "displayName": {
        "en": "Alfa filtered data",
        "ru": "Коэффициент фильтрации данных"
      },
      "description": {
        "en": "Alfa аiltered data",
        "ru": "Коэффициент фильтрации данных"
      },
      "type": "float",
      "defaultValue": 0.0,
      "validValues": [0.1,0.5,1.0],
      "unitMeasured": ""
    }
  ],
  "outputs": [
    {
      "name": "BaroTemp",
      "Schema": {
        "type": "object",
        "id": "http://jsonschema.net",
        "properties": {
          "Pressure": {
            "type": "float",
            "required": true
          },
          "PressureHg": {
            "type": "float",
            "required": true
          },
          "Altitude": {
            "type": "float",
            "required": true
          },
          "Temperature": {
            "type": "float",
            "required": true
          }
        }
      }
    }
  ]
}
